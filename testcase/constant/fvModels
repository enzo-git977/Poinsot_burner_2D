FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

correction
{
    type            DTFLESEnergyCorrection;
    select          all;
}

fixedPower
{
    type            heatSource;
    select          cellSet;
    cellSet         heatSourceSet;

    Q               table
    (
        (0.03        0)
        (0.035       125)
        (0.04        0)
        (10.0        0)
    );          // Set power (W)
}


