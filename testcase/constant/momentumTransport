FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      momentumTransport;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  LES;

LES
{
    model          kEqn;

    delta          cubeRootVol;

    turbulence      on;

    printCoeffs     on;


    cubeRootVolCoeffs
    {
        deltaCoeff      1;
    }
}


